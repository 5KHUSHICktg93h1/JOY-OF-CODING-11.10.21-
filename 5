# JOY-OF-CODING-11.10.21-
#include<stdio.h> int main()
{
int n,a[n],num,i,count=0,attempts=0; printf("Enter the number of chits:\n"); scanf("%d",&n);
printf("Enter the numbers on chits:\n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("Enter the particular number to be picked:\n"); scanf("%d",&num);
for(i=0;i<n;i++)
{
if(num==a[i]) {
count++; attempts=i+1; }
}
printf("Number of duplicate chits : %d\n",count);
printf("Number of attempts : %d",attempts); 
return 0;
}
